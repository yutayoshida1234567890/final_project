<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Events

    <a data-toggle="collapse" href="#restaurant_events_new_form">
      Add event
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @restaurant.events.each do |event| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <!-- Show link start -->
        <a href="/events/<%= event.id %>">
          <%= event.venue_id %>
        </a>
        <!-- Show link end -->

        <!-- Delete link start -->
        <a href="/delete_event_from_venue/<%= event.id %>">
          Delete
        </a>
        <!-- Delete link end -->

      </li>
    <% end %>

    <div class="card-footer">
      <div class="collapse" id="restaurant_events_new_form">
        <!-- New associationcollection form start -->

        <!-- New associationcollection form end -->
      </div>
    </div>

  </ul>
</div>
